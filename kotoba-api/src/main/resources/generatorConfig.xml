<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="context1">
		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection connectionURL="jdbc:mysql://127.0.0.1/test"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root" />

		<javaModelGenerator targetPackage="com.kaungyeehein.kotoba.api.domain"
			targetProject="./src/main/java" />

		<sqlMapGenerator targetPackage="com.kaungyeehein.kotoba.api.mapper"
			targetProject="./src/main/resources" />

		<javaClientGenerator targetPackage="com.kaungyeehein.kotoba.api.mapper"
			targetProject="./src/main/java" type="XMLMAPPER" />

		<table tableName="person" alias="a_person" />
		<table tableName="pet" />

	</context>
</generatorConfiguration>